  "devDependencies": {
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "nyc": "^11.2.1",
    "pre-commit": "^1.2.2",
    "tape": "~1.0.4"
  },
  "scripts": {
    "eslint": "eslint index.js test",
    "test-spec": "tape test/*.js",
    "test": "npm run eslint && nyc npm run test-spec"
